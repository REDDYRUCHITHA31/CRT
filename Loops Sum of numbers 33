#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int sum,n;
    scanf("%d",&n);
    sum=n;
    do
    {
      scanf("%d",&n);
        if(n!=-1)
            sum=sum+n;
    }
    while(n!=-1);
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
