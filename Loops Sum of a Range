#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n,m,i,sum;
    scanf("%d%d",&n,&m);
    sum=0;
    i=n;
    while(i<=m){
        sum=sum+i;
        i++;
    }
    printf("%d",sum);
    return 0;
}
