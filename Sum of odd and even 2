#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int n;
    scanf("%d", &n);
    int a[n];
    int esum = 0, osum = 0;
    for(int i = 0; i < n; i++) 
    { 
        scanf("%d", &a[i]);
        if(a[i] % 2 == 0) 
        { 
            esum += a[i];
        }
        else { osum += a[i];
             }
    }
    printf("%d\n%d\n", osum, esum);
    return 0;
}
